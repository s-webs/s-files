{
    "name": "s-webs/s-files",
    "description": "File manager for Laravel projects",
    "keywords": [
        "laravel",
        "filemanager",
        "file-manager",
        "files",
        "upload",
        "storage"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "S-WEBS",
            "email": "swebs.sh@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "SSystems\\SFiles\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Swebs\\SFiles\\SFilesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
